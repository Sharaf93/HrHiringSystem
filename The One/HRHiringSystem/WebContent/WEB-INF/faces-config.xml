<?xml version="1.0" encoding="UTF-8"?>

<faces-config
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
    version="2.0">
    
	<managed-bean>
		<managed-bean-name>pc_LoginPageAndRegistrationPage</managed-bean-name>
		<managed-bean-class>pagecode.LoginPageAndRegistrationPage</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	
	<managed-bean>
		<managed-bean-name>pc_managerDashboard</managed-bean-name>
		<managed-bean-class>pagecode.ManagerDashboard</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>


	<managed-bean>
		<managed-bean-name>pc_Klkl1</managed-bean-name>
		<managed-bean-class>pagecode.Klkl1</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>pc_klkl</managed-bean-name>
		<managed-bean-class>
			pagecode.PhaseTableMangedBean
		</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>pc_ViewCandidate</managed-bean-name>
		<managed-bean-class>pagecode.ViewCandidate</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>pc_TestPage</managed-bean-name>
		<managed-bean-class>pagecode.TestPage</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>pc_PhasesTable</managed-bean-name>
		<managed-bean-class>pagecode.PhasesTable</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>pc_TrialWebPage</managed-bean-name>
		<managed-bean-class>pagecode.TrialWebPage</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>pc_TrialWebPage1</managed-bean-name>
		<managed-bean-class>pagecode.TrialWebPage1</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	
	<managed-bean>
		<managed-bean-name>pc_CreateNewCandidateForm</managed-bean-name>
		<managed-bean-class>pagecode.CreateNewCandidateForm</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>

	<managed-bean>
		<managed-bean-name>pc_SearchCandidatesPage</managed-bean-name>
		<managed-bean-class>pagecode.SearchCandidatesPage</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>

	<managed-bean>
		<managed-bean-name>pc_HeaderAndNavigationBar</managed-bean-name>
		<managed-bean-class>pagecode.HeaderAndNavigationBar</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>





	<navigation-rule>
		<from-view-id>/LoginPageAndRegistrationPage.xhtml</from-view-id>
		<navigation-case>
			<from-action>#{pc_LoginPageAndRegistrationPage.login}</from-action>
			<from-outcome>successe</from-outcome>
			<to-view-id>/employeesDashboard.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>/LoginPageAndRegistrationPage.xhtml</from-view-id>
		<navigation-case>
			<from-action>#{pc_LoginPageAndRegistrationPage.login}</from-action>
			<from-outcome>successm</from-outcome>
			<to-view-id>/managerDashboard.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>/CreateNewCandidateForm.xhtml</from-view-id>
		<navigation-case>
			<from-action>#{pc_CreateNewCandidateForm.createCandidate}</from-action>
			<from-outcome>success</from-outcome>
			<to-view-id>/ViewCandidate.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>/CreateNewCandidateForm.xhtml</from-view-id>
		<navigation-case>
			<from-action>#{pc_CreateNewCandidateForm.cancelCreation}</from-action>
			<from-outcome>successe</from-outcome>
			<to-view-id>/employeesDashboard.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>/CreateNewCandidateForm.xhtml</from-view-id>
		<navigation-case>
			<from-action>#{pc_CreateNewCandidateForm.cancelCreation}</from-action>
			<from-outcome>successm</from-outcome>
			<to-view-id>/managerDashboard.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>/SearchCandidatesPage.xhtml</from-view-id>
		<navigation-case>
			<from-action>#{pc_SearchCandidatesPage.ViewCandidate}</from-action>
			<from-outcome>success</from-outcome>
			<to-view-id>/ViewCandidate.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>/SearchCandidatesPage.xhtml</from-view-id>
		<navigation-case>
			<from-action>#{pc_SearchCandidatesPage.EditCandidate}</from-action>
			<from-outcome>success</from-outcome>
			<to-view-id>/ViewCandidate.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>/SearchCandidatesPage.xhtml</from-view-id>
		<navigation-case>
			<from-action>#{pc_SearchCandidatesPage.UpdateCandidate}</from-action>
			<from-outcome>success</from-outcome>
			<to-view-id>/ViewCandidate.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	<navigation-rule>
		<from-view-id>/HeaderAndNavigationBar.xhtml</from-view-id>
		<navigation-case>
			<from-action>#{pc_HeaderAndNavigationBar.homepageredirect}</from-action>
			<from-outcome>successe</from-outcome>
			<to-view-id>/employeesDashboard.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	<navigation-rule>
		<from-view-id>/HeaderAndNavigationBar.xhtml</from-view-id>
		<navigation-case>
			<from-action>#{pc_HeaderAndNavigationBar.homepageredirect}</from-action>
			<from-outcome>successm</from-outcome>
			<to-view-id>/managerDashboard.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	<navigation-rule>
		<from-view-id>/HeaderAndNavigationBar.xhtml</from-view-id>
		<navigation-case>
			<from-action>#{pc_HeaderAndNavigationBar.logout}</from-action>
			<from-outcome>success</from-outcome>
			<to-view-id>/LoginPageAndRegistrationPage.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
	<from-view-id>/managerDashboard.xhtml</from-view-id>
	<navigation-case>
	<from-outcome>loginPage</from-outcome>
	<to-view-id>LoginPageAndRegistrationPage.xhtml</to-view-id>
	</navigation-case>
	</navigation-rule>
	
	<lifecycle>
	<phase-listener>phaseListener.AuthorizationListener</phase-listener>
	</lifecycle>
	
</faces-config>